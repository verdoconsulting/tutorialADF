{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-tutorialadf"
		},
		"db_Tutoriales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'db_Tutoriales'"
		},
		"ls_ADSL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ADSL'"
		},
		"ls_BLOB_ADLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_BLOB_ADLS'"
		},
		"ls_ADSL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagetutorialadf.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"db_Tutoriales_connectionString": {
						"value": "[parameters('db_Tutoriales_connectionString')]"
					},
					"ls_ADSL_accountKey": {
						"value": "[parameters('ls_ADSL_accountKey')]"
					},
					"ls_BLOB_ADLS_connectionString": {
						"value": "[parameters('ls_BLOB_ADLS_connectionString')]"
					},
					"ls_ADSL_properties_typeProperties_url": {
						"value": "[parameters('ls_ADSL_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}